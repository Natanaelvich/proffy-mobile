{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "jsx": "react-native",
        "lib": ["dom", "esnext"],
        "moduleResolution": "node",
        "noEmit": true,
        "skipLibCheck": true,
        "resolveJsonModule": true,
        "strict": true,
        "target": "es5",
        "module": "commonjs",
        "allowJs": true,
        "checkJs": true,
        "esModuleInterop": true
    },
    "exclude": ["__tests__/**/*-test.ts"]
}
